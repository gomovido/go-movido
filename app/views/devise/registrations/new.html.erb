
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= f.input :email,
            required: true,
            autofocus: true,
            input_html: { autocomplete: "email"}%>
  <%= f.input :first_name,
            required: true %>
  <%= f.input :last_name,
            required: true %>
  <%= f.button :submit%>
<% end %>
