<div class="pricing-container static-container" data-controller="lead">
  <%= render "pages/pricing/banner",
    headline: "All your contracts in 1 bill and 20% cheaper",
    subheadline: 'Stress-free, in 5mins, and with the option to cancel anytime.',
    background: 'background-color',
    img: 'settle-in-pack-banner-img-phone',
    desktop_img: 'desktop-iphone-banner-settle-in',
    cta_link: onboarding_settle_in_path
  %>
  <%= render "pages/pricing/benefits", title: 'the Settle-in Pack', benefits: @benefits %>
  <%= render 'pages/pricing/services', cta_link: onboarding_settle_in_path, pack: 'Settle-in' %>
  <%= render 'pages/home/how_it_works', slides: @slides %>
  <%= render 'shared/components/faqs', faqs: @faqs %>
  <%= render 'pages/home/contact_banner' %>
  <%= render 'shared/components/testimonials' %>
</div>
