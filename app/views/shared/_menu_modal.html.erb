<div class="modal fade" tabindex="-1" id="menuModal" aria-labelledby="menuModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-fullscreen-sm-down">
    <div class="modal-content">
      <div class="modal-body menu-modal" style="background-image:url(<%= asset_path 'components/menu/background.png' %>);">
        <div class="header">
          <%= image_tag(asset_path("logos/movido-black.svg"), width: 110, class: 'logo', lazy: false) %>
          <div id="hamburger" class="open" type="button" data-bs-dismiss="modal" data-bs-target="#menuModal">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
          </div>
        </div>
        <div class="content">
          <%= render 'shared/modal_links_dashboard' %>
          <%= render 'shared/modal_links' %>
        </div>
      </div>
    </div>
  </div>
</div>
