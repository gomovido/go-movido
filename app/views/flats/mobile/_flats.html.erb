<% flats.map do |flat| %>
  <% if type == 'student_housing' %>
    <%= render 'flats/mobile/uniacco_card', flat: flat, type: type, location: location %>
  <% elsif type == 'entire_flat' %>
    <%= render 'flats/mobile/uniplaces_card', flat: flat, type: type, location: location %>
  <% end %>
<% end %>
